FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Interface_1_airZone
    {
        type            cyclicAMI;
        value           $internalField;
    }

    Interface_1_bladeZone
    {
        type            cyclicAMI;
        value           $internalField;
    }

    airZone_outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

    bladeZone_blade
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
}

// ************************************************************************* //
